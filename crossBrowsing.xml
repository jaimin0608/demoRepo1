<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="orangeHRMSuite" parallel="tests">
	<listeners>
		<listener class-name="util.ExtentListener"></listener>
	</listeners>


	<test name="OrangeHRM-In-firefox">
		<parameter name="browser" value="firefox"></parameter>
		<classes>
			<class name="testCases.LoginPageTest" />
		</classes>
	</test>


	<test name="OrangeHRM-In-edge">
		<parameter name="browser" value="edge"></parameter>
		<classes>
			<class name="testCases.LoginPageTest" />
		</classes>
	</test>
	
	
	<test name="OrangeHRM-In-chrome">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="testCases.LoginPageTest" />
		</classes>
	</test>
</suite>
